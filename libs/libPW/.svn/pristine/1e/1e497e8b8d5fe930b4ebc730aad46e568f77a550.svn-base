#include "GCP.h"
using namespace PW;
GCP::GCP()
{

}
GCP::GCP(int id, double x, double y,double z,QString desc):ID(id),X(x),Y(y),Z(z),mDesc(desc){}

void GCP::setId(int id){
    ID=id;
}
void GCP::setX(double x){
    X=x;
}
void GCP::setY(double y){
    Y=y;
}
void GCP::setZ(double z){
    Z=z;
}
void GCP::setDesc(QString desc){
    mDesc=desc;
}
int GCP::getID(){
    return ID;
}
double GCP::getX(){
    return X;
}
double GCP::getY(){
    return Y;
}
double GCP::getZ(){
    return Z;
}
QString GCP::getDesc(){
    return mDesc;
}
GCP::~GCP()
{

}

