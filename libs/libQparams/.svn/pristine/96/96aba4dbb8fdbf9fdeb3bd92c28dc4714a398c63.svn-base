#include "unit.h"

using namespace QParams;

Unit::Unit(QString code)
{
}

Unit::Unit(QString code, QString name, QString description):
    mCode(code),
    mName(name),
    mDescription(description)
{
}

Unit::Unit(int id, QString code, QString name, QString description):
    mID(id),
    mCode(code),
    mName(name),
    mDescription(description)
{
}

int Unit::getID()
{
    return mID;
}

QString Unit::getCode()
{
    return mCode;
}

QString Unit::getName()
{
    return mName;
}

QString Unit::getDescription()
{
    return mDescription;
}

void Unit::setID(int id)
{
    mID = id;
}

void Unit::setCode(QString code)
{
    mCode = code;
}

void Unit::setName(QString name)
{
    mName = name;
}

void Unit::setDescription(QString description)
{
    mDescription = description;
}

